{
  "name": "cdk",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "cdk": "bin/cdk.js"
  },
    "scripts": {
  "build": "tsc -p tsconfig.json",
  "test": "jest",
"synth": "npx -y aws-cdk@^2 cdk synth"
  "bootstrap:local": "cdklocal bootstrap --require-approval never",
  "deploy:local": "cdklocal deploy --require-approval never",
  "destroy:local": "cdklocal destroy --force",
  "local:up": "docker compose -f ../docker-compose.yml up -d",
  "local:down": "docker compose -f ../docker-compose.yml down -v",
  "local:deploy": "npm run build && npm run bootstrap:local && npm run deploy:local"
},
  "devDependencies": {

    "aws-cdk-local": "^2.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.185.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
